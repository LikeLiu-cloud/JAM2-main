# This is an example input file for Cascade mode

Main:numberOfEvents = 1  ! number of events to generate
Random:seed 2

# This is an example input file. Please change the numberOfEvents and seed when running as needed

Cascade:TimeStepSize = 0.1
Cascade:TimeStep = 200
Cascade:removeSpectator = true

MeanField:mode=14
MeanField:EoS= 32  # =31:NS1,=32:NS2, =33:MD1, =34:MD2, =35:MD4
MeanField:potentialType = 3
#MeanField:gaussWidth = 1.0

#MeanField:mode=14
